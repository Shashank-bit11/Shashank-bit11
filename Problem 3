def generate_series(n):
    series = []
    i = 1
    while len(series) < n:
        series.append(i)
        i += 2
    return series[]
print(generate_series(1))  # Output: [1]
print(generate_series(2))  # Output: [1, 3]
print(generate_series(3))  # Output: [1, 3, 5]
print(generate_series(4))  # Output: [1, 3, 5, 7]
print(generate_series(5))  # Output: [1, 3, 5, 7, 9]
print(generate_series(6))  # Output: [1, 3, 5, 7, 9, 11]
